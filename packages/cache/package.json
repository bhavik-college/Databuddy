{
  "name": "@databuddy/cache",
  "version": "0.0.120",
  "description": "A generic caching implementation for Drizzle ORM that provides automatic query result caching and intelligent cache invalidation",
  "type": "module",
  "private": false,
  "main": "./dist/drizzle.mjs",
  "types": "./dist/drizzle.d.ts",
  "exports": {
    ".": {
      "types": "./dist/drizzle.d.ts",
      "default": "./dist/drizzle.mjs"
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "rm -rf dist && unbuild",
    "vscode:prepublish": "bun run build"
  },
  "keywords": [
    "cache",
    "caching",
    "drizzle",
    "drizzle-orm",
    "redis",
    "query-cache",
    "database",
    "orm",
    "typescript"
  ],
  "license": "MIT",
  "homepage": "https://databuddy.cc",
  "repository": {
    "type": "git",
    "url": "https://github.com/databuddy-analytics/databuddy.git",
    "directory": "packages/cache"
  },
  "bugs": {
    "url": "https://github.com/databuddy-analytics/databuddy/issues"
  },
  "dependencies": {
    "drizzle-orm": "^0.45.1"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
